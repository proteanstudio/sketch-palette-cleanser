function onDocumentChanged(context) {
  var document = Document.getSelectedDocument();
  var storedColorPathDict = Settings.documentSettingForKey(document, 'color-path-dictionary');
  if (storedColorPathDict === undefined) return;
  var changePaths = Array.from(context.actionContext).map(function (actionContext) {
    return actionContext.fullPath().toString().trim();
  });
  var changePath = changePaths[0];
  var changePathSegments = changePath.match(/([a-z])\w+/g);
  var isNewSharedStyle = changePath.includes('layerStyles') && changePathSegments.length === 1;
  var isSharedStyleRename = changePath.includes('layerStyles') && changePathSegments.length === 2;
  var colorVarChangePath = changePaths.find(function (path) {
    return path.includes('sharedSwatches');
  });
  var isColorVarChange = !!colorVarChangePath;

  if (isNewSharedStyle) {
    changePath = (_babel_runtime_helpers_readOnlyError__WEBPACK_IMPORTED_MODULE_1___default()("changePath"), changePaths[1]);
    changePathSegments = (_babel_runtime_helpers_readOnlyError__WEBPACK_IMPORTED_MODULE_1___default()("changePathSegments"), changePath.match(/([a-z])\w+/g));
  }

  var finalSegment = changePathSegments[changePathSegments.length - 1];
  var actionContext = context.actionContext[0];

  var _changePath$match = changePath.match(/\d+/g),
      _changePath$match2 = _babel_runtime_helpers_slicedToArray__WEBPACK_IMPORTED_MODULE_0___default()(_changePath$match, 2),
      abIndex = _changePath$match2[1];

  var hasPopulatedPalette = !!storedColorPathDict && Object.keys(storedColorPathDict).length > 0;
  var isMove = actionContext.isMove && actionContext.isMove();
  var actionType = actionContext.type();
  var isDeletion = actionType === 2 && !isMove;
  var layerDepthArr = changePathSegments.filter(function (segment) {
    return segment === 'layers';
  });

  if (isSharedStyleRename) {
    Object(_handleSharedStyleRename__WEBPACK_IMPORTED_MODULE_7__["default"])(changePath, document);
    return;
  }

  if (isColorVarChange) {
    Object(_handleColorVarChange__WEBPACK_IMPORTED_MODULE_8__["default"])(changePath, document, colorVarChangePath, storedColorPathDict);
    return;
  }

  if (!hasPopulatedPalette || layerDepthArr.length < 2 || abIndex === '0') return;

  if (finalSegment === 'sharedStyleID' || finalSegment === 'layer') {
    Object(_handleLayerAssignment__WEBPACK_IMPORTED_MODULE_4__["default"])(changePath, storedColorPathDict, document, isNewSharedStyle);
    return;
  }

  if (finalSegment === 'color' || finalSegment === 'enabled') {
    var modifiedCP = finalSegment === 'enabled' ? "".concat(changePath.split('enabled')[0], "color") : changePath;
    Object(_handleChangedColor__WEBPACK_IMPORTED_MODULE_5__["default"])(modifiedCP, storedColorPathDict, document);
    return;
  }

  if (isMove && changePaths.length > 1) {
    Object(_handleLayerHierarchyChange__WEBPACK_IMPORTED_MODULE_6__["default"])(changePaths, storedColorPathDict, document);
    return;
  }

  if (isDeletion) {
    Object(_handleLayerDeletion__WEBPACK_IMPORTED_MODULE_3__["default"])(changePath, storedColorPathDict, document);
    return;
  }
}
